{
  "name": "@chezmoi-sh/argocd-ui-extension-application-map",
  "version": "0.1.1",
  "private": false,
  "dependencies": {
    "@dagrejs/dagre": "^1.1.4",
    "@xyflow/react": "^12.6.0",
    "graphology": "^0.26.0",
    "graphology-types": "^0.24.8"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "webpack": "webpack --config ./webpack.config.js",
    "webpack:watch": "webpack --config ./webpack.config.js --watch",
    "dev": "NODE_ENV=development pnpm webpack:watch",
    "build:dev": "NODE_ENV=development pnpm webpack",
    "build:prod": "NODE_ENV=production pnpm webpack",
    "build:enpack": "tar -C dist -cvf dist/extension.tar resources",
    "build": "pnpm build:prod && pnpm build:enpack",
    "start": "pnpm dev"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^7.1.2",
    "esbuild-loader": "^4.3.0",
    "raw-loader": "^0.5.1",
    "sass": "^1.34.1",
    "sass-loader": "^10.2.1",
    "style-loader": "^1.3.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^4.7.2"
  }
}
